{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "wyadfv2"
		},
		"ADLSGen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://wyadls2.dfs.core.windows.net"
		},
		"KeyStore_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://wykv.vault.azure.net/"
		},
		"SQLDB_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "SQLDBSecret"
		},
		"SQLDW_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "SQLDWSecret"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"KeyStore_properties_typeProperties_baseUrl": {
						"value": "[parameters('KeyStore_properties_typeProperties_baseUrl')]"
					},
					"ADLSGen2_properties_typeProperties_url": {
						"value": "[parameters('ADLSGen2_properties_typeProperties_url')]"
					},
					"SQLDB_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('SQLDB_properties_typeProperties_connectionString_secretName')]"
					},
					"SQLDW_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('SQLDW_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}